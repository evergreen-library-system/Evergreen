    <eg-grid-action handler="grid_actions.retarget_wide" group="[% l('Hold') %]"
      label="[% l('Find Another Target') %]"></eg-grid-action>
    <eg-grid-action handler="grid_actions.cancel_wide_hold" group="[% l('Hold') %]"
