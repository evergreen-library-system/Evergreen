[% 
# $locale is encoded as aa_bb.  The IDL needs aa-BB.
SET idl_locale = '';
IF ctx.locale;
  idl_locale = ctx.locale.split('_').0 _ '-' _ ctx.locale.split('_').1.upper;
END;
%]

