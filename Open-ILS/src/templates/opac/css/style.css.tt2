    text-align: center;
}

#adv_search_parent {
    margin-bottom:0px;
}

#myopac_loading {
    width:100%;
    text-align:center;
    padding-top:20px;
    font-size: [% css_fonts.size_bigger %];
    font-weight:bold;
}

.chili_link {
    width:100px !important;
    text-align: center !important;
}

.chili_review div.chili_link div {
