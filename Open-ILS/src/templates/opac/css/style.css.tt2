/* Toggled items. */
/* A block that is initially hidden and shown by clicking an anchor. */
[id^="toggled-block-"]{
    display: none;
}
[id^="toggled-block-"]:target{
    display: block;
}
/* Inline text that is initially hidden and shown by clicking an anchor. Useful on a span. */
[id^="toggled-inline-"]{
    display: none;
}
[id^="toggled-inline-"]:target{
    display: inline;
}

