}

table.acct_notes {
    border-collapse: collapse;
    margin: -1em 0 0.5em 0;
}

table.acct_notes * {
    border: solid thin;
    padding: 0.5em; 
}

table.acct_notes th {
    font-weight: bold;
}

#myopac_sum_fines {
    float:right;
    padding: 15px 0px 0px 23px;
    background: [% css_colors.accent_ultralight %];
    width: 177px;
    height: 166px;
}

#myopac_sum_fines_placehold { float: right; width: 177px; height: 166px; }

.acct_holds_temp td {
