    font-size: [% css_fonts.size_small %];
    text-transform:uppercase;
    padding-right: 5px;
}

.adv_search_font {
    font-size: [% css_fonts.size_smaller %];
}

.search_catalog_lbl {
    font-size: [% css_fonts.size_bigger %];
}

.lbl1 {
    font-size: [% css_fonts.size_bigger %];
    font-weight:bold;
